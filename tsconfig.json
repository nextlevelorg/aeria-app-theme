{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "types": [
      "vite/client"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
